  "additionalProperties": false,
  "required": [
    "SqlConnectionString",
    "BlobStorageConnectionString",
	"GovUkNotifyApiKey",
	"TlevelQueriedSupportEmailAddress",
	"TechnicalSupportEmailAddress",
	"FeedbackEmailAddress",
	"BlobStorageSettings",
	"DataProtectionSettings"
    "DfeSignInSettings",
	"ResultsAndCertificationInternalApiSettings"
  ],
  "type": "object"
}