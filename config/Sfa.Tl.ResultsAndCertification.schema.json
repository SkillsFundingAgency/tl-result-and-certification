    "SqlConnectionString": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "SQLServiceAccountConnectionString"
    },
    "BlobStorageConnectionString": {
      "type": "string",
      "environmentVariable": "BlobStorageConnectionString"
    },
    "BlobStorageAccountName": {
      "type": "string",
      "environmentVariable": "BlobStorageAccountName"
    },
    "BlobStorageDataProtectionContainer": {
      "type": "string",
      "environmentVariable": "BlobStorageDataProtectionContainer"
