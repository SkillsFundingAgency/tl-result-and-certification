      "additionalProperties": false,
      "required": [
        "MetadataAddress",
        "ClientId",
        "ClientSecret",
        "CallbackPath",
        "SignedOutCallbackPath",
        "LogoutPath",
		"ProfileUrl",
		"Timeout",
        "Issuer",
        "Audience",
        "ApiSecret",
        "ApiUri",
        "TokenEndpoint",
        "Authority",
