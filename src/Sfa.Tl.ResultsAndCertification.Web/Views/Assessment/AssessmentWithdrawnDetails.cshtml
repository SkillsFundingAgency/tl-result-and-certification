@model Sfa.Tl.ResultsAndCertification.Web.ViewModel.Assessment.Manual.AssessmentUlnWithdrawnViewModel
@using Sfa.Tl.ResultsAndCertification.Web.Content.Assessment

@{
    ViewData["Title"] = @AssessmentWithdrawnDetails.Page_Title;
}

@section NavigationBar {
    <vc:back-link model=Model.BackLink></vc:back-link>
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

        <h1 class="govuk-heading-l">@AssessmentWithdrawnDetails.Learner_Has_Been_Withdrawn_Heading</h1>
        <p class="govuk-body">@AssessmentWithdrawnDetails.Learner_Registration_Withdrawn_Info</p>


        <dl class="govuk-summary-list govuk-summary-list--no-border govuk-!-margin-top-7">
            <vc:summary-item model=Model.SummaryLearnerName></vc:summary-item>
            <vc:summary-item model=Model.SummaryUln></vc:summary-item>
            <vc:summary-item model=Model.SummaryDateofBirth></vc:summary-item>
            <vc:summary-item model=Model.SummaryProvider></vc:summary-item>
            <vc:summary-item model=Model.SummaryTlevelTitle></vc:summary-item>
        </dl>

        <div class="govuk-button-group">
            <a id="searchAgainButton" class="govuk-button" role="button" asp-route="SearchAssessments">@AssessmentWithdrawnDetails.Button_Search_Again</a>
            <a id="backToHomeButton" class="govuk-button govuk-button--secondary" role="button" data-module="govuk-button" asp-route="Home">@AssessmentWithdrawnDetails.Button_Back_To_Home</a>
        </div>
    </div>
</div>