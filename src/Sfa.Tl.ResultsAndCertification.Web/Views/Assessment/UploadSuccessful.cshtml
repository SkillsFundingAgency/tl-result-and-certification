@using Sfa.Tl.ResultsAndCertification.Web.Content.Assessment
@model Sfa.Tl.ResultsAndCertification.Web.ViewModel.Assessment.UploadSuccessfulViewModel

@{
    ViewData["Title"] = @UploadSuccessful.Page_Title;
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="govuk-panel govuk-panel--confirmation">
            <h1 class="govuk-panel__title">@UploadSuccessful.Section_Heading</h1>
        </div>
        <p class="govuk-body govuk-!-padding-top-3">@Model.SuccessfulAssessmentEntriesText</p>
        <p class="govuk-!-margin-top-8"><a asp-route="AssessmentDashboard" class="govuk-button" role="button">@UploadSuccessful.Back_To_Assessment_Entries_Button_Text</a></p>
    </div>
</div>