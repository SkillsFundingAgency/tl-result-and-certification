@model Sfa.Tl.ResultsAndCertification.Web.ViewModel.Dashboard.DashboardViewModel

@using Sfa.Tl.ResultsAndCertification.Web.Content.Dashboard
@using Sfa.Tl.ResultsAndCertification.Common.Enum

@{
    ViewData["Title"] = CommonDashboard.Page_Title;
}

@switch (Model.LoginUserType)
{
    case LoginUserType.AwardingOrganisation:
        <partial name="_aoDashboard" />
        break;

    case LoginUserType.TrainingProvider:
        <partial name="_providerDashboard" />
        break;

    case LoginUserType.Admin:
        <partial name="_adminDashboard" />
        break;
}