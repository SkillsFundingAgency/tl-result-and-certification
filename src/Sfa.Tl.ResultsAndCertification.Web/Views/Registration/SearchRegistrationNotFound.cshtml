@model Sfa.Tl.ResultsAndCertification.Web.ViewModel.Registration.Manual.UlnNotFoundViewModel
@using Sfa.Tl.ResultsAndCertification.Web.Content.Registration

@{
    ViewData["Title"] = @SearchRegistrationNotFound.Page_Title;
}

@section NavigationBar {
    <vc:back-link model=Model.BackLink></vc:back-link>
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="govuk-form-group">
            <h1 class="govuk-label-wrapper">
                <label class="govuk-label govuk-label--l govuk-!-margin-bottom-6">
                    @string.Format(@SearchRegistrationNotFound.Uln_Cannot_Be_Found_Text, @Model.Uln)
                </label>
            </h1>
            <p class="govuk-body">@SearchRegistrationNotFound.If_you_Think_That_The_Uln_Should_Exists_Text</p>
            <ul class="govuk-list govuk-list--bullet">
                <li>@SearchRegistrationNotFound.Is_Registered_With_Another_Ao_Text</li>
                <li>@SearchRegistrationNotFound.Has_Never_Been_Registered_With_Us_Text</li>
            </ul>
        </div>
        <p>
            <a class="govuk-button" role="button" asp-route="SearchRegistration">@SearchRegistrationNotFound.Back_To_Search_Link_Text</a>
        </p>
    </div>
</div>