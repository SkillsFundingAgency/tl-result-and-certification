@{
    ViewData["Title"] = "Registrations upload unsuccessful page";
}


<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

        <h1 class="govuk-heading-xl">
            Registrations upload unsuccessful
        </h1>
        <p class="govuk-body govuk-inset-text">The file could not be uploaded as it contains errors. Download the error report to find out why some registrations were unsuccessful.</p>

        <section>
            <div class="gem-c-attachment__thumbnail">
                <a class="govuk-link" target="_self" tabindex="-1" aria-hidden="true" href="#">
                    <svg class="gem-c-attachment__thumbnail-image" version="1.1" viewBox="0 0 99 140" width="99" height="140" aria-hidden="true">
                        <path d="M12 12h75v27H12zM12 59h9v9h-9zM12 77h9v9h-9zM12 95h9v9h-9zM12 113h9v9h-9zM30 59h57v9H30zM30 77h39v9H30zM30 95h57v9H30zM30 113h48v9H30z" stroke-width="0"></path>
                    </svg>
                </a>
            </div>
            <div>
                <h2 class="govuk-!-font-size-27 govuk-!-font-weight-regular">
                    <a class="govuk-link" target="_self" href="#">Download error report</a>
                </h2>
                <p class="govuk-body-s">CSV, 19.5 KB</p>
            </div>
        </section>
    </div>

    <div class="govuk-grid-column-two-thirds">
        <p class="govuk-!-margin-top-5"><a class="govuk-button" role="button" href="/1-13/AO/upload-file.html">Back to upload file</a></p>
    </div>
</div>