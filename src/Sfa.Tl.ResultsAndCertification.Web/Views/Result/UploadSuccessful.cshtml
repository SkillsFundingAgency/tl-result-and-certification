@using Sfa.Tl.ResultsAndCertification.Web.Content.Result
@model Sfa.Tl.ResultsAndCertification.Web.ViewModel.Result.UploadSuccessfulViewModel

@{
    ViewData["Title"] = @UploadSuccessful.Page_Title;
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="govuk-panel govuk-panel--confirmation">
            <h1 class="govuk-panel__title">@UploadSuccessful.Section_Heading</h1>
        </div>
        <p class="govuk-body govuk-!-padding-top-3">@Model.SuccessfulResultText</p>
        <p class="govuk-!-margin-top-8"><a id="backToResultsButton" asp-route="ResultsDashboard" class="govuk-button" role="button">@UploadSuccessful.Back_To_Results_Button_Text</a></p>
    </div>
</div>